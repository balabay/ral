алг цел many_params(цел a, цел b, цел c, цел d)
нач
	знач := a + b + c + d
	вывод a, ' ', b, ' ', c, ' ', d, нс
кон

алг цел сравнения()
нач
цел а = 10
вещ б = 8.9

вывод "а = ", а, " б = ", б, " а == б: ", а = б, " а != б: ", а != б, нс
вывод "а = 10.0:", а = 10.0, нс
вывод "а = 10.1:", а = 10.1, нс
вывод "(а = 10.1) = 0:", (а = 10.1) = 0, нс
вывод "б = 8:", б = 8, нс
вывод "б = 8.9:", б = 8.9, нс

вывод "а < 10:", а < 10, нс
вывод "а <= 10:", а <= 10, нс
вывод "а > 10:", а > 10, нс
вывод "а >= 10:", а >= 10, нс

вывод "б < 10:", б < 10, нс
вывод "б <= 10:", б <= 10, нс
вывод "б <= б:", б <= б, нс
вывод "б > 10:", б > 10, нс
вывод "б >= 10:", б >= 10, нс
вывод "б >= 8.9:", б >= 8.9, нс

вывод "(б < 10) == (а = 10):", (б < 10) = (а = 10), нс
вывод "б < 10 = а = 0:", б < 10 = а = 0, нс

кон

